@model B2WTI.PCFTI.INFRAESTRUTURA.TRANSVERSAL.DTO.Modulo.Cadastro.ViewModel.AnoCalendarioView
@{
    ViewBag.Title = "Novo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <div class="wrapper wrapper-content animated fadeInUp">
            <div class="ibox">
                <div class="ibox-title">
                    <h5>Criar novo Status</h5>
                </div>
                <div class="ibox-content">

                    @using (Html.BeginForm())
                    {
                        Html.EnableClientValidation(true);

                        <div class="form-horizontal">

                            <div class="form-group">
                                @Html.LabelFor(model => model.Ano, htmlAttributes: new { @class = "col-sm-2 control-label" })
                                <div class="col-sm-10">
                                    @Html.EditorFor(model => model.Ano, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.Ano, "", new { @class = "text-danger" })
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-sm-4 col-sm-offset-2">
                                    <button class="btn btn-primary" type="submit"><i class="fa fa-check"></i>Cadastrar o novo Ano</button>
                                </div>
                            </div>

                        </div>
                    }

                    <div>
                        @Html.ActionLink("Voltar", "Index")
                    </div>



                </div>
            </div>
        </div>
    </div>
</div>